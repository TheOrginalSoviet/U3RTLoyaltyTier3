local features = window:AddTab("Fast Glitch") -- Name of tab
features:Show() -- shows the tab

   local switch = features:AddSwitch("Jungle Rock", function(bool)
	 -- toggle_god_mode(bool)
   local player = game.Players.LocalPlayer
local runService = game:GetService("RunService")

local followEnabled = bool -- Toggle this to enable/disable following

repeat wait() until player.Character and player.Character:FindFirstChild("HumanoidRootPart")
local hrp = player.Character:WaitForChild("HumanoidRootPart")
local model = workspace.machinesFolder:FindFirstChild("Ancient Jungle Rock")

if model and model:IsA("Model") then
    -- Set PrimaryPart if not already set
    if not model.PrimaryPart then
        model.PrimaryPart = model:FindFirstChildWhichIsA("BasePart")
    end

    -- Scale down all parts by 0.1x
    if model.PrimaryPart then
        for _, part in ipairs(model:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Size = part.Size * 0.3
                part.Position = model.PrimaryPart.Position + (part.Position - model.PrimaryPart.Position) * 0.1
            elseif part:IsA("MeshPart") then
                part.Size = part.Size * 0.3
            end
        end
    end

    -- Follow logic
    runService.RenderStepped:Connect(function()
        if followEnabled and model.PrimaryPart then
            local newCFrame = hrp.CFrame * CFrame.new(0, 0, -9)
            model:SetPrimaryPartCFrame(newCFrame)
        end
    end)
else
    warn("Muscle King Mountain model not found or is not a Model")
end

wait(0.1)
local player = game.Players.LocalPlayer
repeat task.wait() until player.Character
local character = player.Character
local humanoid = character:WaitForChild("Humanoid")
local replicatedStorage = game:GetService("ReplicatedStorage")
local muscleEvent = player:WaitForChild("muscleEvent")
local tool = player.Backpack:FindFirstChildOfClass("Tool") or character:FindFirstChildOfClass("Tool")

if not tool then
    warn("Tool not found!")
    return
end

local attackTime = tool:WaitForChild("attackTime")
local animFolder = replicatedStorage:WaitForChild("gameAnims"):WaitForChild("Tools"):WaitForChild("Punch")
local idleAnim = humanoid:LoadAnimation(animFolder:WaitForChild("idle"))
local attackAnims = {}
for _, anim in pairs(animFolder:WaitForChild("attacks"):GetChildren()) do
    if anim:IsA("Animation") then
        table.insert(attackAnims, humanoid:LoadAnimation(anim))
    end
end

-- Play idle anim when equipped
tool.Equipped:Connect(function()
    idleAnim:Play()
end)

tool.Unequipped:Connect(function()
    idleAnim:Stop()
    for _, anim in pairs(attackAnims) do
        anim:Stop()
    end
end)

-- Auto punch loop
task.spawn(function()
    local lastAttack = 0
    local currentAnim = nil

    while true do
        if tool.Parent == character and humanoid.Health > 0 then
            local now = tick()
            if now - lastAttack >= attackTime.Value then
                lastAttack = now

                -- Pick a random animation (different from last)
                local choices = {}
                for _, anim in pairs(attackAnims) do
                    if anim ~= currentAnim then
                        table.insert(choices, anim)
                    end
                end
                currentAnim = #choices > 0 and choices[math.random(1, #choices)] or currentAnim

                if currentAnim then
                    currentAnim:Play()
                    local name = currentAnim.Animation.Name
                    local hit = name == "rightAttack" and "rightHand" or (name == "leftAttack" and "leftHand" or "rightHand")
                    muscleEvent:FireServer("punch", hit)
                end
            end
        end
        task.wait()
    end
end)
wait(0.1)
game.Players.LocalPlayer.Backpack.Punch.attackTime.Value = 0.000001
wait(0.1)
game.ReplicatedStorage.strengthFrame.Visible = bool
game.ReplicatedStorage.durabilityFrame.Visible = bool
game.ReplicatedStorage.agilityFrame.Visible = bool
game.ReplicatedStorage.evilKarmaFrame.Visible = bool
game.ReplicatedStorage.goodKarmaFrame.Visible = bool
game.ReplicatedStorage.gemsFrame.Visible = bool
game.ReplicatedStorage.tokensFrame.Visible = bool
wait(0.1)
loadstring(game:HttpGet("https://raw.githubusercontent.com/CasperFlyModz/discord.gg-rips/main/FPSBooster.lua"))()

end)
switch:Set(false)

