game.Players.LocalPlayer.PlayerGui.updatesMenuGui.Frame.Visible = bool
	game.Players.LocalPlayer.PlayerGui.updatesMenuGui.Frame.Size = UDim2.new(0.7, 0, 0.3, 0)
		game.Players.LocalPlayer.PlayerGui.updatesMenuGui.Frame.Position = UDim2.new(0.2, 0, 0.3, 0)
end)
switch:Set(false)
