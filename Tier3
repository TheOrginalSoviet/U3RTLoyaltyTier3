features:AddButton("Early Update",function()	
local player = game.Players.LocalPlayer
local gui = player:WaitForChild("PlayerGui")

-- Currency Frame Elements
local currencyFrameGui = gui:WaitForChild("currencyFrameGui")
local currencyFrame = currencyFrameGui:WaitForChild("currencyFrame")
local limitedStockButton = currencyFrame:WaitForChild("limitedStockButton")
local rewardButton = currencyFrame:WaitForChild("rewardButton")
local inviteButton = currencyFrame:WaitForChild("inviteButton")
local inviteLabel = inviteButton:WaitForChild("buttonLabel")
inviteLabel.Text = "ATTACKS"

-- Limited Stock UI
local limitedStockGui = gui:WaitForChild("limitedStockGui")
local limitedStockMenu = limitedStockGui:WaitForChild("limitedStockMenu")
local limitedStockClose = limitedStockMenu:WaitForChild("xButton")

-- Updates Menu
local updatesMenuGui = gui:WaitForChild("updatesMenuGui")
local updatesMenu = updatesMenuGui:FindFirstChild("updatesMenu") or updatesMenuGui:FindFirstChild("Frame")
local updatesMenuClose = updatesMenu:WaitForChild("xButton")

-- Invite Friends UI
local inviteFriendsGui = gui:WaitForChild("inviteFriendsGui")
local inviteRewardsMenu = inviteFriendsGui:WaitForChild("inviteRewardsMenu")
local inviteClose = inviteRewardsMenu:WaitForChild("xButton")

-- Right Side Offer Button
local rightSideGui = gui:WaitForChild("rightSideGui")
local specialOfferButton = rightSideGui:WaitForChild("specialOfferButton")

-- === Initial UI Setup ===
limitedStockButton.Visible = true
rewardButton.Visible = true
specialOfferButton.Visible = true

limitedStockGui.Enabled = true
limitedStockMenu.Visible = false

inviteFriendsGui.Enabled = true
inviteRewardsMenu.Visible = false

updatesMenuGui.Enabled = true
updatesMenu.Visible = false

-- === Button Connections ===

-- Limited Stock Button
limitedStockButton.MouseButton1Click:Connect(function()
	limitedStockMenu.Visible = true
end)

limitedStockClose.MouseButton1Click:Connect(function()
	limitedStockMenu.Visible = false
end)

-- Updates Button
rewardButton.MouseButton1Click:Connect(function()
	updatesMenu.Visible = true
end)

updatesMenuClose.MouseButton1Click:Connect(function()
	updatesMenu.Visible = false
end)

-- Invite Friends Button
inviteButton.MouseButton1Click:Connect(function()
	inviteRewardsMenu.Visible = true
end)

inviteClose.MouseButton1Click:Connect(function()
	inviteRewardsMenu.Visible = false
end)

-- Toggle updates menu on inviteButton again? (if intended)
-- Uncomment this if you want inviteButton to also toggle updatesMenu
-- inviteButton.MouseButton1Click:Connect(function()
-- 	print('fail')
-- end)


end)
features:AddButton("Speed Grind Multiplier",function()
	local player = game.Players.LocalPlayer
local replicatedStorage = game:GetService("ReplicatedStorage")

-- Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "GrindGui"
screenGui.ResetOnSpawn = false
screenGui.Parent = player:WaitForChild("PlayerGui")

-- Create Main Frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 250, 0, 120)
mainFrame.Position = UDim2.new(0, 20, 0.5, -60)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
mainFrame.BorderSizePixel = 0
mainFrame.Parent = screenGui

-- Create Title Label
local title = Instance.new("TextLabel")
title.Name = "Title"
title.Size = UDim2.new(1, 0, 0.3, 0)
title.Position = UDim2.new(0, 0, 0, 0)
title.BackgroundTransparency = 1
title.Text = "Speed Grind Multiplier"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.TextScaled = true
title.Font = Enum.Font.SourceSansBold
title.Parent = mainFrame

-- Create TextBox
local textBox = Instance.new("TextBox")
textBox.Size = UDim2.new(0.8, 0, 0.25, 0)
textBox.Position = UDim2.new(0.1, 0, 0.4, 0)
textBox.PlaceholderText = "Enter reps per frame"
textBox.Text = "1"
textBox.TextScaled = true
textBox.Font = Enum.Font.SourceSans
textBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
textBox.Parent = mainFrame

-- Info Label
local infoLabel = Instance.new("TextLabel")
infoLabel.Size = UDim2.new(1, 0, 0.2, 0)
infoLabel.Position = UDim2.new(0, 0, 0.8, 0)
infoLabel.BackgroundTransparency = 1
infoLabel.Text = "Decimal = spread across frames"
infoLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
infoLabel.TextScaled = true
infoLabel.Font = Enum.Font.SourceSansItalic
infoLabel.Parent = mainFrame

-- Rep loop logic
local repsPerFrame = 1
local accumulated = 0

textBox.FocusLost:Connect(function()
	local input = tonumber(textBox.Text)
	if input and input >= 0.1 then
		repsPerFrame = input
	else
		textBox.Text = tostring(repsPerFrame)
	end
end)

game:GetService("RunService").RenderStepped:Connect(function()
	accumulated += repsPerFrame
	while accumulated >= 1 do
		replicatedStorage.muscleEvent:FireServer("rep")		accumulated -= 1
	end
end)

end)
